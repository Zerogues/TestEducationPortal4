<h3>GeneratorLuckyNumber</h3>
<br/>

<hr/>

<div class="test-success h3">Title</div>

<button @onlick="GenerateNumber" class="btn btn-dark">Generate Lucky Number</button>

@if (LuckyNumber > 0)
{
	<p class="P-2">LuckyNumber - @LuckyNumber</p>
}
@code {
	public int LuckyNumber { get; set; } = 0;

	private void GenerateNumber()
	{
		Random rnd = new();
		LuckyNumber = rnd.Next(1, 100);
	}
}
